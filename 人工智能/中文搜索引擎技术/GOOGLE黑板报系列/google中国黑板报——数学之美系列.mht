Content-Disposition: inline; filename=mods.css
Content-Type: text/css; name=mods.css
Content-Location: http://hi.baidu.com/ui/css/mods.css
Content-Transfer-Encoding: 8bit

#m_comment p { margin:0pt; }
#m_comment .cnt img { border:0px; }
#m_comment .date { color:#666666; }

#m_event .nav{text-align:right}
#m_event .nav span { color:#000000; font-weight:bold; }
#m_event .nav span.lk { color:#3399CC; cursor:pointer; text-decoration:underline; font-weight:normal; }
#m_event td{height:30px;vertical-align:middle;padding-left:5px;}
#m_event .alter td{background-color:#f0f0f0;}
#m_event .time{color:#666;width:100px}
#m_event .line{border-top:1px solid #ECECEC;margin-top:3px;line-height:3px;height:3px;}

#m_friends { overflow: hidden; }
#m_friends .frd { float:left;  margin:0px 3px; width:75px; height:90px; overflow:hidden; }
#m_friends .portrait { background-position:center bottom; background-repeat:no-repeat; height:55px; display:block; }
#m_friends .info { text-align:center; }
#m_friends .info a { padding-left:16px; background-position:left top; background-repeat:no-repeat; }
#m_friends .online a { background-image:url(http://img.baidu.com/passport/icn_online.gif); }
#m_friends .offline a { background-image:url(http://img.baidu.com/passport/icn_offline.gif); }
#m_friends .hosttip { height:90px;line-height:90px;}


#m_pro .wrapper { clear:both; overflow:hidden; }
#m_pro .portrait { width:110px;float:left; text-align:center;margin-right:6px;}
#m_pro .userinfo{color:#333333;font-size:12px;font-weight:normal;padding-left:6px;}
#m_pro a.btfan{background:url(http://img.baidu.com/hi/img/mf/btfanbg.gif) no-repeat;width:88px;height:22px;line-height:22px;display:block;margin-right:10px;padding:2px 0 0 0;text-align:center;margin-top:10px;}
#m_pro div.line{margin-top:10px;line-height:5px;}

#m_useronline .frd { float:left;  margin:0px 3px; width:75px; height:90px; overflow:hidden; }
#m_useronline .portrait { background-position:center bottom; background-repeat:no-repeat; height:55px; display:block; }
#m_useronline .info { text-align:center; }

/* profile page */
#warpper { position:relative;margin:0 0 0 200px; }
#warpper .content { position:absolute;left:-200px;top:5px;}
#warpper .photo { position:absolute;left:-200px;top:5px; width:190px; text-align:center; }
#warpper .line { margin-left:auto; margin-right:auto; }
#warpper .act { padding-left:5px; padding-right:5px }

.clear { clear:both; height:1px; line-height:1px; font-size:0px; }

/* for 好友升级 */
#m_friendStat th,#m_friendStat td{vertical-align:top;padding:5px 0 5px 5px;}
#m_friendStat td{font:400 14px/22px arial;border-bottom:1px solid #D2E9F4;}
#m_friendStat td span{zoom:1;margin-left:10px;color:#8c8c8c;font-size:12px;}
#m_friendStat td img{border:1px solid #a1ccdd;vertical-align:middle;margin-right:10px;}
#m_friendStat td a img{border:1px solid #a1ccdd;vertical-align:bottom;margin-right:10px;}
#m_friendStat .pg{height:30px;font:400 14px/30px arial;text-align:center;padding-top:10px;;}

#m_searchFriend td{height:28px;}

